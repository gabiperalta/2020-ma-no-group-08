    <div id="accordion">

        {{#each licitaciones}}
            {{#each this.presupuestos}}
                <div class="card">
                    <div class="card-header" id="headingPresupuesto{{@index}}-{{../operacionEgreso.identificador}}">
                        <h5 class="mb-0">
                            <button class="btn btn-link" data-target="#collapsePresupuesto{{@index}}-{{../operacionEgreso.identificador}}" data-toggle="collapse" aria-expanded="false" aria-controls="collapsePresupuesto{{@index}}-{{../operacionEgreso.identificador}}">
                                Presupuesto {{ identificador}} - Egreso {{../operacionEgreso.identificador}}
                            </button>
                        </h5>
                    </div>

                    <div id="collapsePresupuesto{{@index}}-{{../operacionEgreso.identificador}}" class="collapse" aria-labelledby="headingPresupuesto{{@index}}-{{../operacionEgreso.identificador}}" data-parent="#accordion">
                        <div class="container">
                            <div class="row">

                                <div class="card-body col-10">
                                    Items:
                                    <ul>
                                        {{#each this.items}}
                                            <li>{{ descripcion }} - ${{valor}}</li>
                                        {{/each}}
                                    </ul>
                                </div>
                                <div class="col my-auto">
                                    <button type="button" class="btn btn-primary">Categorizar</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            {{/each}}
        {{/each}}

